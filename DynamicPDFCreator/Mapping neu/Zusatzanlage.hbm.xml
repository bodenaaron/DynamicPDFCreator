<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns = "urn:nhibernate-mapping-2.2" assembly="DynamicPDFCreator" namespace="DynamicPDFCreator">

  <class name = "Zusatzanlage" table="tblZusatzanlagen">
    <id name = "id" column="id">
      <generator class = "identity"/>
    </id>
    <property name="anlage" type="string">
      <column name="anlage" sql-type="NVARCHAR(255)" />
    </property>
    <many-to-one name = "idPDF" class="PDF"/>

  </class>
</hibernate-mapping>